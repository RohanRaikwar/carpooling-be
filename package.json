    "@aws-sdk/client-s3": "^3.980.0",
    "@aws-sdk/lib-storage": "^3.980.0",
    "@mapbox/polyline": "^1.2.1",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "axios": "^1.13.3",
    "bcryptjs": "^3.0.3",
    "bullmq": "^5.67.1",
    "connect-timeout": "^1.9.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "firebase-admin": "^13.6.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongodb": "^7.0.0",
    "mongoose": "^9.0.0",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "nodemailer": "^7.0.11",
    "opossum": "^9.0.0",
    "redis": "^5.10.0",
