{
  "name": "carpooling-backend",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev:server": "NODE_ENV=staging nodemon --watch 'src/**/*.ts' --exec 'node --loader ts-node/esm' src/server.ts",
    "dev:worker": "NODE_ENV=staging nodemon --watch 'src/**/*.ts' --exec 'node --loader ts-node/esm' src/modules/mail/mail.worker.ts",
    "dev:sms-worker": "NODE_ENV=staging nodemon --watch 'src/**/*.ts' --exec 'node --loader ts-node/esm' src/modules/sms/sms.worker.ts",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:worker\" \"npm run dev:sms-worker\"",
    "build": "tsc && prisma generate",
    "start": "NODE_ENV=production node dist/server.js",
    "start:staging": "NODE_ENV=staging node dist/server.js",
    "start:worker": "NODE_ENV=production node dist/modules/mail/mail.worker.js",
    "start:worker:staging": "NODE_ENV=staging node dist/modules/mail/mail.worker.js",
    "start:sms-worker": "NODE_ENV=production node dist/modules/sms/sms.worker.js",
    "start:sms-worker:staging": "NODE_ENV=staging node dist/modules/sms/sms.worker.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:migrate:reset": "prisma migrate reset",
    "prisma:studio": "prisma studio",
    "db:push": "prisma db push",
    "postinstall": "prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
